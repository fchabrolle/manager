  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/manager-telephony.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "start": "lerna run build --stream --scope='@ovh-ux/manager-telephony' --include-filtered-dependencies",
    "start:dev": "lerna run dev --stream --scope='@ovh-ux/manager-telephony' --include-filtered-dependencies",
    "start:watch": "lerna run dev:watch --stream --parallel --scope='@ovh-ux/manager-telephony' --include-filtered-dependencies",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.0-beta.5"
  },
  "peerDependencies": {
    "@ovh-ux/manager-core": "^1.1.1",
    "@ovh-ux/manager-telecom-styles": "^2.0.0",
    "@ovh-ux/telecom-universe-components": "^1.5.1",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.7.5",
    "angular-translate": "^2.18.1",
    "oclazyload": "^1.1.0",
    "ovh-ui-angular": "^2.21.4",
    "ovh-ui-kit": "^2.23.1",
    "ovh-ui-kit-bs": "^2.0.1"
  }
